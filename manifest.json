{
  "name": "github syntax",
  "version": "0.3.0",
  "manifest_version": 2,
  "icons": {
    "128": "icon-128.png"
  },
  "description": "Adds syntax highlighting to github's split diff pull request view.",
  "content_scripts": [{
    "js": [
      "lib/jquery.min.js",
      "codediff.js",
      "difflib.js",
      "jquery.appear.js",
      "highlightjs/highlight.pack.js",
      "highlight.user.js"
    ],
    "css": [
      "highlight.css",
      "highlightjs/styles/googlecode.css"
    ],
    "matches": [
      "https://github.com/*"
    ],
    "run_at": "document_idle"
  }]
}
